{
  "name": "pdf-crop dev",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "workspaceFolder": "/workspaces/pdf-crop",
  "remoteUser": "vscode",
  "postCreateCommand": "go mod download",
  "customizations": {
    "vscode": {
      "settings": {
        "go.useLanguageServer": true,
        "go.toolsManagement.autoUpdate": true,
        "go.formatTool": "gofmt"
      },
      "extensions": [
        "golang.go"
      ]
    }
  }
}
